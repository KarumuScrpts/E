	for i,v in pairs(PLAYERS) do
		local PCHAR = _PLAYERS[v].Character
		if PCHAR:FindFirstChild('Humanoid') then
			PCHAR.Humanoid.Health = PCHAR.Humanoid.MaxHealth
		end
