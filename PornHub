local ADMINS = {}
local BANS = {}

function _G.ADD_ADMIN(ID) table.insert(ADMINS, ID) end
function _G.ADD_BAN(ID) table.insert(BANS, ID) end

local VERSION = '1.8.0'
local UPDATED = '3/15/2017'
local CHANGELOG = {
	' * updated ;nolimbs',
	' * updated ;rhats',
