--basically makes a background
MakeShadow = function(GUI, Deepness)
	for i=1, Deepness do
		local G = GUI:Clone()
		G.ZIndex = GUI.ZIndex - 1
		G.Size=UDim2.new(1,12,1,12)
		G.Position=UDim2.new(0,-6,0,-6)
		G.Active=false
		G.Draggable=false
		G:ClearAllChildren()
		G.Name = GUI.Name.." Shadow"
		G.Parent = GUI
	end
end
