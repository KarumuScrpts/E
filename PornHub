function(ARGS, SPEAKER)
	local PLAYERS = GET_PLAYER(ARGS[1], SPEAKER)
	for i,v in pairs(PLAYERS) do
		local PCHAR = _PLAYERS[v].Character
		RESET_MODEL(PCHAR)
		UPDATE_MODEL(PCHAR, _PLAYERS[v].Name)
	end
end)

