nouni=false
local function lo(l)
loadstring(game:HttpGet(l,true))()
nouni=true
end

if game.PlaceId==142823291 then --Murder Mystery 2
lo("https://raw.githubusercontent.com/ApolloHub/AutoLoader/main/mm2.txt")
end


if nouni==false then --Universal
lo("https://raw.githubusercontent.com/ApolloHub/AutoLoader/main/Universal")
end
